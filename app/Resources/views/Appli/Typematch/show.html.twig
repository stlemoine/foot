{% extends '::Appli/layout.html.twig' %}

{% block title %} {{ parent() }}|typematch|{{'grid.show'|trans}} {% endblock %}

{% block appli_content %}

    <h1>
        {{'menu.typematch'|trans}} 
        <a href="{{ path('typematch') }}" id="retour" name="retour" class="btn btn-danger btn-xs">{{ 'retour.liste' | trans }}</a>
    </h1>

    <table class="table table-bordered table-condensed">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
        </tbody>
    </table>

{% endblock appli_content %}
