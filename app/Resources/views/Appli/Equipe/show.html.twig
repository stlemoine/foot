{% extends '::Appli/layout.html.twig' %}

{% block title %} {{ parent() }}|equipe|{{'grid.show'|trans}} {% endblock %}

{% block appli_content %}

    <h1>
        {{'menu.equipe'|trans}} 
        <a href="{{ path('equipe') }}" id="retour" name="retour" class="btn btn-danger btn-xs">{{ 'retour.liste' | trans }}</a>
    </h1>

    <table class="table table-bordered table-condensed">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Club_id</th>
                <td>{{ entity.Clubid }}</td>
            </tr>
            <tr>
                <th>Saison_id</th>
                <td>{{ entity.saisonid }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ entity.photo }}</td>
            </tr>
            <tr>
                <th>Scoring_id</th>
                <td>{{ entity.scoringid }}</td>
            </tr>
        </tbody>
    </table>

{% endblock appli_content %}
