{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Equipe</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Club_id</th>
                <td>{{ entity.Clubid }}</td>
            </tr>
            <tr>
                <th>Saison_id</th>
                <td>{{ entity.saisonid }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ entity.photo }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('equipe') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('equipe_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
